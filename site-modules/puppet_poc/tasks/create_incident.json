{    "description": "Create Incident  from Puppet",
    "input_method": "stdin",
    "supports_noop": true,
    "parameters": {
        "snow_server": {
            "description": "Service now server",
            "type": "String",
            "default": "https://dev218806.service-now.com"
        },
        "inc_ep": {
                "description": "Service now Endpoint",
                "type": "String",
                "default": "/api/now/table/incident"
            },
        "sysparm_display_value": {
                "description": "sysparm_display_value",
                "type": "Boolean",
                "default": true
            },
        "sysparm_exclude_reference_link": {
                "description": "sysparm_exclude_reference_link",
                "type": "Boolean",
                "default": true
            },
        "use_ssl": {
                "description": "Use SSL - true/false",
                "type": "Boolean",
                "default": true
            },
        "sysparm_fields": {
                "description": "sysparm_fields",
                "type": "String",
                "default": "number,short_description,sys_created_on,state,sys_created_by,impact,priority,caller_id,description"
        
        },
        "token": {
            "description": "Token Value",
            "type": "String",
            "default": "Basic YWRtaW46Q2g9YURKbko4ZDMl",
            "sensitive": true
        }
    }
}
